class Solution {
    public long solution(int a, int b) {
        long answer = 0;
        if(a==b)
        {
            answer = (long)a;
        }
        else if(a<b)
        {
            for(int i=a;i<b+1;i++)
            {
                answer+=(long)i;
            }
        }
        else
        {
            for(int i=b;i<a+1;i++)
            {
                answer +=(long)i;
            }
        }
        return answer;
    }
}
